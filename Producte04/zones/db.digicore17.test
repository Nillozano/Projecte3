;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1-digicore17.test usuari.digicore17.test. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;name server NS records
	INS	NS	ns1.digicore-17.test.

; registers
ns1	IN	A	192.168.4.17
server	IN	A	192.168.4.17
dbserverIN	A	192.168.4.117
data	IN	CNAME	dbserver
